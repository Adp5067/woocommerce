{
	"root": "packages/js/@woocommerce/api/",
	"sourceRoot": "packages/js/@woocommerce/api/src",
	"projectType": "library",
	"targets": {
		"changelog": {
			"executor": "./tools/executors/changelogger:changelog",
			"options": {
				"action": "add",
				"cwd": "packages/js/@woocommerce/api"
			}
		},
		"build": {
			"executor": "@nrwl/workspace:run-script",
			"options": {
				"script": "build"
			}
		},
		"clean": {
			"executor": "@nrwl/workspace:run-script",
			"options": {
				"script": "clean"
			}
		},
		"compile": {
			"executor": "@nrwl/workspace:run-script",
			"options": {
				"script": "compile"
			}
		},
		"lint": {
			"executor": "@nrwl/workspace:run-script",
			"options": {
				"script": "lint"
			}
		},
		"test": {
			"executor": "@nrwl/workspace:run-script",
			"options": {
				"script": "test"
			}
		}
	}
}
